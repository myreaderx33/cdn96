    <script>

        var formMag = $("#__AjaxAntiForgeryMagInfoForm"),
            tokenMag = $('input[name="__RequestVerificationToken"]', formMag).val();
        $(document).ready(function() {
            $(".image-viewer").click(function() {
                $(this).parent().find("img").viewer("show");
            }), $("#searchIn li").click(function(e) {
                e.preventDefault();
                var a = $(this).data("val"),
                    i = $("#q").val();
                if (i.length < 2)
                    $("#q").tooltip("enable"), $("#q").addClass("border-danger")
                        .attr("data-original-title", "برای جستجو حداقل باید دو حرف وارد کنید.").tooltip("fixTitle")
                        .tooltip("show");
                else
                    switch (a) {
                    case 1:
                        if ($('#isExact').is(':checked'))
                            window.location = '/searchinpapers/?adv=false&isExact=true&cols=1,2,3&ew=' + i;
                        else
                            window.location = '/searchinpapers/?q=' + i;
                        break;
                    case 2:
                        if ($('#isExact').is(':checked'))
                            window.location = '/searchinpapers/?dt=3&adv=False&isExact=true&cols=1,2,3&ew=' + i;
                        else
                            window.location = '/searchinpapers/?dt=3&q=' + i;
                    break;
                    case 3:
                        if ($('#isExact').is(':checked'))
                           window.location = '/searchinnewspapers/?dt=3&adv=True&isExact=true&cols=1,2,3&ew=' + i;
                        else
                           window.location = '/searchinnewspapers/?q=' + i;
                    break;
                    case 4:
                        window.location = '/searchinmagazines/?iq=' + i;
                        break;
                    case 6:
                        window.location = '/searchinpapers/?adv=False&cols=4&s=1&ew=' + i;
                        break;
                    case 7:
                        window.location = '/searchinnewspapers/?adv=True&cols=1,2&s=1&np=0&p=' + i;
                        break;
                    case 5:
                        var o = $(this).data("id"),
                            n = $(this).data("magazine");
                        window.location = 1 == n
                            ? '/searchinpapers/?m=' + o + "&q=" + i
                            : '/searchinnewspapers/?np=' + o + "&q=" + i;
                    }
            }), $(".nice-select").niceSelect(), $("body").persianNum(), $("#q").keyup(function() {
                $(this).val().length > 1 && ($(this).removeClass("border-danger"), $(this).tooltip("dispose"));
            }), $("#lastVolumes").change(function() {
                var e = $(this).val();
                e && (window.location = '/newspapertoc/' + e);
            });
        });

    </script>
